{
  "name": "@breezbook/backend-airtable",
  "version": "1.0.0",
  "module": "dist/esm/index.js",
  "type": "module",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "npx tsc -p tsconfig.json",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest watch",
    "start": "nodemon dist/esm/index.js",
    "db:migrate": "dotenv -e .env.production.local -- bash -c 'npx postgrator --config=migrations/schema/postgratorrc.json'",
    "db:migrate-local": "npx postgrator --config=migrations/schema/postgratorrc.json",
    "db:migrate-carwash": "dotenv -e .env.production.local -- bash -c 'npx postgrator --config=migrations/data/carwash/postgratorrc.json'",
    "db:migrate-carwash-local": "npx postgrator --config=migrations/data/carwash/postgratorrc.json",
    "dev": "nodemon -x \"npm run buildrun\"",
    "buildrun": "npm run compile && npm run server",
    "build-run-db": "npm run compile && npm run bounce:db",
    "bounce:db": "node dist/cjs/devDb.js",
    "compile": "npx tsc -p tsconfig.json",
    "server": "NODE_ENV=development node dist/esm/index.js",
    "clean:postgres": "./scripts/run-clean-postgres.sh && npm run db:migrate-local",
    "make:schema-types": "pg-to-ts generate --conn postgresql://user:password@127.0.0.1:5432/postgres --schema public --output src/generated/dbtypes.ts"
  },
  "devDependencies": {
    "@testcontainers/postgresql": "^10.4.0",
    "@types/child-process-promise": "^2.2.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "child-process-promise": "^2.2.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.28.0",
    "pg-to-ts": "^4.1.1",
    "postgrator-cli": "^7.0.0",
    "typescript": "5.3.3",
    "vite": "5.0.7",
    "vitest": "0.33.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "uuid": "^9.0.1"
  }
}
